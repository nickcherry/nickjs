{
  "name": "create-nickjs",
  "version": "0.1.22",
  "type": "module",
  "license": "MIT",
  "author": "Nick Cherry",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickcherry/nickjs"
  },
  "main": "src/cli/index.ts",
  "bin": {
    "create-nick": "src/cli/index.ts"
  },
  "scripts": {
    "clean:bootstrap-artifacts": "bun run src/server/bin/clean/bootstrapArtifacts.ts",
    "dev": "bun run src/server/bin/dev.ts",
    "format": "bunx prettier --write \"**/*.{ts,tsx,js,json}\" --config ./.prettierrc.cjs --ignore-path .prettierignore",
    "generate:db-types": "bun run src/server/bin/generate/dbTypes.ts",
    "lint": "bunx eslint src",
    "migrate:to-latest": "bun run src/server/bin/migrate/toLatest.ts && bun run generate:db-types",
    "migrate:reset": "bun run src/server/bin/migrate/reset.ts",
    "serve": "bun src/server/bin/serve.ts",
    "test": "bun test src",
    "typecheck": "bunx tsc -p tsconfig.json --noEmit",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/autoprefixer": "^10.2.0",
    "@types/chalk": "^2.2.0",
    "@types/classnames": "^2.3.1",
    "@types/compressible": "^2.0.2",
    "@types/cssnano": "^5.1.0",
    "@types/helmet": "^4.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.16",
    "@types/react-frame-component": "^4.1.6",
    "@types/tailwindcss": "^3.1.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "bun-types": "^1.0.11",
    "eslint": "latest",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-no-restricted-imports": "^0.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "kysely-codegen": "^0.11.0",
    "terminate": "^2.6.1",
    "tsc-watch": "^6.0.4",
    "type-fest": "^4.7.1"
  },
  "dependencies": {
    "@elysiajs/static": "^0.7.1",
    "@planetscale/database": "^1.11.0",
    "@sinclair/typebox": "^0.31.28",
    "@tusbar/cache-control": "^1.0.1",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.3.0",
    "classnames": "^2.3.2",
    "compressible": "^2.0.18",
    "cssnano": "^6.0.1",
    "elysia": "^0.7.29",
    "elysia-helmet": "^1.0.1",
    "elysia-rate-limit": "^2.0.1",
    "elysia-requestid": "^1.0.9",
    "helmet": "^7.1.0",
    "kysely": "^0.26.3",
    "kysely-planetscale": "^1.4.0",
    "postcss": "^8.4.31",
    "prism-react-renderer": "^2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1"
  },
  "keywords": [
    "typescript",
    "bun",
    "elysia",
    "kysely",
    "planetscale",
    "react"
  ]
}
